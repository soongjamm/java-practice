<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="koreaTire" class="ch07.IoC_DI.c5_di_spring_property.KoreaTire"></bean>
    <bean id="americaTire" class="ch07.IoC_DI.c5_di_spring_property.AmericaTire"></bean>
    <bean id="car" class="ch07.IoC_DI.c5_di_spring_property.Car">
        <property name="tire" ref="koreaTire"></property>
    </bean>
</beans>